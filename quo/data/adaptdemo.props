# RSS/QuO
# Specify location of kernel.conf
org.cougaar.rss.properties=/opt/cougaar/quo/data/kernel.conf

# Allow Quo Kernel Guis for each Cougaar Node
org.cougaar.lib.quo.kernel.gui=true

# Link Protocols
org.cougaar.message.transport.aspects=org.cougaar.lib.mquo.RemoteSSLAspect
org.cougaar.message.protocol.classes=org.cougaar.mts.std.SSLRMILinkProtocol,org.cougaar.mts.std.RMILinkProtocol,org.cougaar.mts.std.LoopbackLinkProtocol

# Aspects
org.cougaar.message.transport.aspects=org.cougaar.lib.mquo.QuoProxyAspect,org.cougaar.lib.mquo.RemoteSSLAspect,org.cougaar.lib.mquo.TrafficMaskAspect,org.cougaar.mts.std.StatisticsAspect

# Debug
org.cougaar.message.transport.debug=none

# Compression
org.cougaar.lib.quo.wrapper=org.cougaar.lib.mquo.CompressWrapper



