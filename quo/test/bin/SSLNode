#!/bin/sh

ssl="-Djavax.net.ssl.trustStore=$HOME/keystore -Djavax.net.ssl.trustStorePassword=password -Djavax.net.ssl.keyStore=$HOME/keystore -Djavax.net.ssl.keyStorePassword=password"


protocols="-Dorg.cougaar.message.protocol.classes=org.cougaar.core.mts.SSLRMILinkProtocol,org.cougaar.core.mts.LoopbackLinkProtocol"

#debug="-Djavax.net.debug=ssl"
debug="-Dorg.cougaar.message.transport.debug=policy"

COUGAAR_JAVA_ARGS="$protocols $ssl $debug"
export COUGAAR_JAVA_ARGS


exec ${COUGAAR_INSTALL_PATH}/core/bin/Node $*



