#!/bin/sh

protocols="-Dorg.cougaar.message.protocol.classes=org.cougaar.core.mts.LoopbackLinkProtocol,org.cougaar.lib.quo.CorbaLinkProtocol"
orb="-Dorg.omg.CORBA.ORBClass=org.jacorb.orb.ORB -Dorg.omg.CORBA.ORBSingletonClass=org.jacorb.orb.ORBSingleton"
security="-Dorg.cougaar.message.security=org.cougaar.core.society.DummyMessageSecurityManager"
aspects="-Dorg.cougaar.message.transport.debug=aspects -Dorg.cougaar.message.transport.aspects=org.cougaar.core.mts.SerializationAspect,org.cougaar.core.mts.SequenceAspect,org.cougaar.core.mts.SecurityAspect"

COUGAAR_JAVA_ARGS="$protocols $aspects $security $orb"
export COUGAAR_JAVA_ARGS


exec ${COUGAAR_INSTALL_PATH}/core/bin/Node $*

