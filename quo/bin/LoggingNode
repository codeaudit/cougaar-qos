#!/bin/sh

logfile=${LOGFILE:-$1.trace}
COUGAAR_JAVA_ARGS="-Dorg.cougaar.message.transport.aspects=org.cougaar.lib.quo.LoggingAspect -Dorg.cougaar.lib.quo.logfile=$logfile"
export COUGAAR_JAVA_ARGS

# The field-separator can be set in org.cougaar.lib.quo.separator
# (default=space)

# The application tag can be set in org.cougaar.lib.quo.tag
# (default=COUGAAR).
# Use ALP for spyglass import.



exec ${COUGAAR_INSTALL_PATH}/core/bin/Node $*
