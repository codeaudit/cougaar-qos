#!/bin/sh

# Serialization aspect prints status at INFO level of logging service,
# not DEBUG level.

logging=-Dorg.cougaar.core.logging.config.filename=$COUGAAR_INSTALL_PATH/quo/bin/props/cougaar/logging.props
aspects="-Dorg.cougaar.message.transport.aspects=org.cougaar.core.mts.SerializationAspect"

COUGAAR_JAVA_ARGS="$logging $aspects"
export COUGAAR_JAVA_ARGS


exec ${COUGAAR_INSTALL_PATH}/core/bin/Node $*

