#!/bin/sh


# Test the combination of artifical message scrambling plus sequence
# numbers, to se if the sequencing will unscramble messages properly.
# The sequence numbers must be added befoe the scrambling, so the
# sequence aspect must follow the scramble aspect.

COUGAAR_JAVA_ARGS="-Dorg.cougaar.message.transport.aspects=org.cougaar.core.mts.ScrambleAspect,org.cougaar.core.mts.SequenceAspect,org.cougaar.core.mts.WasteCPUAspect"
export COUGAAR_JAVA_ARGS


exec ${COUGAAR_INSTALL_PATH}/core/bin/Node $*

